view,
text,
navigator,
input,
image,
swiper,
swiper-item,
scroll-view,
page {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

page {
  --themeColor: #ff2d4a;

  --black: #000000;
  --semi: #353535;
  --grep: #888888;
  --light: #b2b2b2;
  font-size: 28rpx;
}

image {
  width: 100%;
  display: block;
}
